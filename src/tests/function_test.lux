
fun count(n) {
  
  if (n > 1) {
    count(n - 1);
  }

  print(n);
}

fun count2(n)  {
  while (n < 10) {
    if (n == 3) {
      print(n);
      return n;
      } // <--
    print(n);
    n = n + 1;
  }
}
