
fun count(n) {
  
  if (n > 1) {
    count(n - 1);
  }

  println(n);
}

fun count2(n)  {
  while (n < 10) {
    if (n == 3) {
      println(n);
      return n;
      } // <--
    println(n);
    n = n + 1;
  }
}
